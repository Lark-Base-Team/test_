{
  "name": "excel_compare_and_import",
  "version": "2.0.0",
  "type": "module",
  "description": "This is a Base extension for Feishu that compares local Excel files with base and imports the differences into the current Base.",
  "scripts": {
    "dev": "vite --mode dev",
    "dev:Beta": "vite --mode beta",
    "github:build": "vite build --base /Excel-Compare-and-Import/ --mode alpha",
    "build": "vite build --mode stable",
    "preview": "vite preview --port 4173",
    "createData": "npx vite-node ./scripts/createTestData.ts -n",
    "prepare": "husky install",
    "style": "npx prettier . --write",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:u": "vitest -u",
    "typecheck": "vitest typecheck"
  },
  "keywords": [
    "Excel",
    "Base",
    "Bitable",
    "Import",
    "Compare"
  ],
  "author": {
    "name": "qww",
    "url": "https://github.com/497363983"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zjut-bio-party-undergraduate-branch/Excel-Compare-and-Import.git"
  },
  "dependencies": {
    "@lark-base-open/js-sdk": "^0.3.4",
    "@qww0302/use-bitable": "^0.1.2",
    "@unhead/vue": "^1.7.3",
    "@vueuse/core": "^10.3.0",
    "dayjs": "^1.11.9",
    "element-plus": "^2.4.0",
    "mathjs": "^11.11.0",
    "vue": "^3.3.4",
    "vue-i18n": "^9.6.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.4.10",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitest/coverage-v8": "^0.34.4",
    "@vitest/ui": "^0.34.4",
    "commander": "^11.0.0",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "mockjs": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "typescript": "^5.1.6",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^3.0.1",
    "vitest": "^0.34.4",
    "vue-tsc": "^1.8.8"
  },
  "lint-staged": {
    "*.{js,css,md,ts,vue,json}": "prettier --write"
  },
  "bugs": {
    "url": "https://github.com/zjut-bio-party-undergraduate-branch/Excel-Compare-and-Import/issues"
  },
  "homepage": "https://github.com/zjut-bio-party-undergraduate-branch/Excel-Compare-and-Import#readme"
}
